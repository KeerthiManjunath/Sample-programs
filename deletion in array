#include <stdio.h>

int main()
{
    int arr[]={1,0,2,3,0,4};
    for(int i=0;i<6;i++)
    {
        if(arr[i]==0)
        {
           continue;
           
        }
        else
        {
            printf("%d ",arr[i]);
        }
    }
    // for(int i=0;i<6;i++)
    // printf("%d ",arr[i]);
}
